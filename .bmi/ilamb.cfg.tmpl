# Benchmark CMIP5-compatible model outputs against all available benchmarks.

[h1: {simulation_name}]
bgcolor  = "#FFECE6"

#-----------------------------------------------------------------------

[h2: Gross Primary Production]
variable = "gpp"
cmap = "Greens"
mass_weighting = True

[Fluxnet]
source = "DATA/gpp/FLUXNET/gpp.nc"
weight = 9
table_unit = "g m-2 d-1"
plot_unit = "g m-2 d-1"

[Fluxnet-MTE]
source = "DATA/gpp/FLUXNET-MTE/gpp_0.5x0.5.nc"
weight = 15
table_unit = "Pg yr-1"
plot_unit = "g m-2 d-1"
space_mean = False
skip_iav = True
